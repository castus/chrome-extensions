{
 "name": "Auto-Translate",
 "version": "1.0",
 "description": "Automatically translate selection text by goolge translate",
 "background_page": "translate.html",
 "permissions": [
  "tabs", 
  "http://ajax.googleapis.com/*",
  "http://*.google.com/*"
 ],
 "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/tranlation.png"
 },
 "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["script.js"],
    "all_frames": "true"
  }]
}
